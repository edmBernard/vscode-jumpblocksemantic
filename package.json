{
    "name": "jumpblocksemantic",
    "displayName": "JumpBlockSemantic",
    "description": "Shortcuts to jump through files by Semantic block. Faster navigation of functions and logical blocks. (support C++ and Python)",
    "version": "0.0.2",
    "publisher": "edmBernard",
    "author": {
        "name": "Erwan BERNARD"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/edmBernard/vscode-jumpblocksemantic"
    },
    "engines": {
        "vscode": "^1.8.0"
    },
    "dependencies": {
        "bindings": "~1.5.0",
        "node-addon-api": "^8.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:jumpBlockSemantic.up",
        "onCommand:jumpBlockSemantic.down",
        "onCommand:jumpBlockSemantic.out"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "jumpBlockSemantic.up",
                "title": "Jump to previous sibling Block"
            },
            {
                "command": "jumpBlockSemantic.down",
                "title": "Jump to next sibling Block"
            },
            {
                "command": "jumpBlockSemantic.out",
                "title": "Jump to parent Block"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@types/vscode": "*",
        "eslint": "^9.39.1",
        "typescript": "^5.9.3"
    },
    "gypfile": true
}
